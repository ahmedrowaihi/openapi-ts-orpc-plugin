{
  "name": "@ahmedrowaihi/openapi-ts-orpc",
  "version": "0.1.0",
  "description": "oRPC plugin for @hey-api/openapi-ts - Generate type-safe RPC clients and servers from OpenAPI specs",
  "keywords": [
    "openapi",
    "openapi-ts",
    "orpc",
    "rpc",
    "typescript",
    "codegen",
    "type-safe",
    "plugin"
  ],
  "homepage": "https://github.com/ahmedrowaihi/openapi-ts",
  "bugs": {
    "url": "https://github.com/ahmedrowaihi/openapi-ts/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ahmed Rowaihi",
    "email": "ahmedrowaihi@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedrowaihi/openapi-ts.git",
    "directory": "openapi-ts-orpc-plugin"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "biome format --write .",
    "format:check": "biome check --write .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@hey-api/openapi-ts": ">=0.92.0",
    "@orpc/client": "^1.13.0",
    "@orpc/contract": "^1.13.0",
    "@orpc/openapi": "^1.13.0",
    "@orpc/openapi-client": "^1.13.0",
    "@orpc/server": "^1.13.0",
    "@orpc/tanstack-query": "^1.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.1",
    "@hey-api/codegen-core": "^0.7.0",
    "@hey-api/openapi-ts": "^0.92.0",
    "@hey-api/shared": "^0.2.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
